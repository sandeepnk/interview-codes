#include<iostream>

class Box{

public: int height;
		int width;
		int area();
};

int Box::area(){

	return height*width;
}

int main(){

	Box *b = new Box;
	b->height = 20;
	b->width = 10;
	std::cout << "Area>>> "<< b->area();
}


