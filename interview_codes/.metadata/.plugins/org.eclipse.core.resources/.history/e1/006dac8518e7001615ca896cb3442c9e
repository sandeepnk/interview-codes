#include<iostream>

using namaspace std;

class Box{

public: int height;
		int width;
		int area();
};

Box::int area(){

	return height*width;
}

int main(){

	Box b1;
	b1.height = 20;
	b1.width = 10;
	cout<< "Area>>> "<< area();
}


